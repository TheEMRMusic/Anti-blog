---
title: example
layout: 'base_rootstyle.njk'
---

<article>
<div>
  
    <h1>
      {{ title }}
    </h1>
    <h2>
      {{ subtitle }}
    </h2>
    <h4 class="article_date">
      {{  page.date | postDate  }}
    </h4>
    <figure>
      <img src="{{ image }}" alt="{{ imageAlt }}">
      <figcaption>
        {{ imageAlt }}
      </figcaption>
    </figure>

  <div class="content">

    <desc>
      {{ description }}
    </desc>
    
    {{ content | safe }}

    </div>
  
</div>
</article>

